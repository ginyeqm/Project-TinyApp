<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
    <!-- Add your EJS code here -->
  <% include partials/_header %>
  <% for(var url in urls) { %>
    <div>
      <p style="display: inline-block;"><a href="/u/<%=url%>"><%=url%></a> -> <%=urls[url].longUrl%></p>

      <a href= "/urls/<%=url%>">
        <input type ="submit" value="Edit" ></input></a>

      <form action= "/urls/<%=url%>/delete" method ="POST" style="display: inline-block;">
        <input type ="submit" value="delete" ></input>
      </form>

    </div>
  <% } %>

</body>
</html>


